{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "import/no-dynamic-require":0,
        "max-len": 0,
        "no-use-before-define":0,
        "radix": [1,"as-needed"]
    }
}
